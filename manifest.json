{
  "name": "YourType",
  "description": "For development and design use only. Only works with <h1>, <h2>, <h3>, <h4>, <h5>, <h6>, <p>, <span>, <ul>, <li>, <label>, and <a> elements. If page elements have styles defined by their classes and ids, YourType may not work depending on specificity rating.",
  "version": "0.0.0",
  "permissions": [
    "activeTab"
  ],
  "icons": { 
    "48": "assets/48_logo.png",
    "128": "assets/128_logo.png",
    "256": "assets/256_logo.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "YourType",
    "default_icon": "assets/19_logo.png"
  },
  "manifest_version": 2
}
